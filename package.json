    "react": "^16.12.0"
  },
  "devDependencies": {
    "@types/has-ansi": "^3.0.0",
    "@types/jest": "^24.9.0",
    "@types/lodash.isequal": "^4.5.5",
    "@types/node": "^13.1.8",
    "@types/react": "^16.9.17",
    "@typescript-eslint/parser": "^2.16.0",
    "eslint": "^6.8.0",
